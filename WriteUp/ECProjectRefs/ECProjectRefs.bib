	keywords = {classification, deep   learning, facial expression recognition, feature fusion, in the wild, library, texture features},
	pages = {061407},
	file = {Sun et al. - 2016 - Facial expression recognition in the wild based on.pdf:files/822/Sun et al. - 2016 - Facial expression recognition in the wild based on.pdf:application/pdf}
}

