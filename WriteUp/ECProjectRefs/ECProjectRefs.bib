@article{midwood_changes_2012,
	title = {Changes in aquatic vegetation and fish communities following 5 years of sustained low water levels in coastal marshes of eastern {Georgian} {Bay}, {Lake} {Huron}},
	volume = {18},
	issn = {1354-1013},
	doi = {10.1111/j.1365-2486.2011.02558.x},
	abstract = {Aquatic vegetation in the relatively pristine coastal wetlands of eastern Georgian Bay provides critical habitat for a diverse fish community. Declining water levels in Lake Huron over the past decade, however, have altered the wetland plant assemblages in favour of terrestrial (emergent and meadow) taxa and have thus reduced or eliminated this important ecosystem service. In this study, we compared IKONOS satellite images for two regions of eastern Georgian Bay (acquired in 2002 and 2008) to determine significant changes in cover of four distinct wetland vegetation groups [meadow (M), emergent (E), high-density floating (HD) and low-density floating (LD)] over the 6years. While LD decreased significantly (mean -2995.4m2), M and HD increased significantly (mean +2020.9m2 and +2312.6m(2), respectively) between 2002 and 2008. Small patches of LD had been replaced by larger patches of HD. These results show that sustained low water levels have led to an increasingly homogeneous habitat and an overall net loss of fish habitat. A comparison of the fish communities sampled between 2003 and 2005 with those sampled in 2009 revealed that there was a significant decline in species richness. The remaining fish communities were also more homogeneous. We suggest that the observed changes in the wetland plant community due to prolonged low water levels may have resulted in significant changes in the fish communities of coastal wetlands in eastern Georgian Bay.},
	language = {English},
	number = {1},
	journal = {Global Change Biology},
	author = {Midwood, Jonathan D. and Chow-Fraser, Patricia},
	month = jan,
	year = {2012},
	note = {WOS:000298598900009},
	keywords = {habitat use, st-lawrence-river, biotic   integrity, change detection, coastal wetlands, global climate-change, Great Lakes, laurentian great-lakes, long-term trends, predation risk, remote sensing, remote-sensing data, species-diversity, water-level regulation, wetland change},
	pages = {93--105},
	file = {Midwood and Chow-Fraser - 2012 - Changes in aquatic vegetation and fish communities.pdf:/Users/looi/Zotero/storage/EEAZVMGN/Midwood and Chow-Fraser - 2012 - Changes in aquatic vegetation and fish communities.pdf:application/pdf}
