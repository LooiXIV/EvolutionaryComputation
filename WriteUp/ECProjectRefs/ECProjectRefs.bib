	file = {Full Text PDF:/Users/looi/Zotero/storage/GSHM4TIB/Anderson and Benke - 1994 - Growth and reproduction of the cladoceran Ceriodap.pdf:application/pdf;Snapshot:/Users/looi/Zotero/storage/I49H9FQH/abstract.html:text/html}
}

