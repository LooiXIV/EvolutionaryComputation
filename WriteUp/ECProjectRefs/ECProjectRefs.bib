	pmid = {17473775},
	keywords = {Humans, Running, Temperature, Weather, Adult, Cold Temperature, Female, Hot Temperature, Male, Physical Endurance, Physical Fitness, Prospective Studies, Sex Factors, Time Factors},
	pages = {487--493},
