	month = sep,
	year = {2014},
	keywords = {0496 Water quality, 0497 Wetlands, 1820 Floodplain dynamics, 4845 Nutrients and nutrient cycling, hydrologic connectivity, perirheic zone},
	pages = {7394--7405},
	file = {Jones et al. - 2014 - Perirheic mixing and biogeochemical processing in .pdf:/Users/looi/Zotero/storage/GZWZ53XM/Jones et al. - 2014 - Perirheic mixing and biogeochemical processing in .pdf:application/pdf;Snapshot:/Users/looi/Zotero/storage/M8749CB6/abstract\;jsessionid=2E5AE58573A847B65982C0231FFC1193.html:text/html}
