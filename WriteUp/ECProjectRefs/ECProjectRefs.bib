	month = jan,
	year = {2007},
	keywords = {collaborative research, interdisciplinary research, philosophy},
	pages = {55--64},
	file = {Eigenbrode et al. - 2007 - Employing Philosophical Dialogue in Collaborative .pdf:/Users/looi/Zotero/storage/S6G6CQ59/Eigenbrode et al. - 2007 - Employing Philosophical Dialogue in Collaborative .pdf:application/pdf;Snapshot:/Users/looi/Zotero/storage/KV8DPD58/55.html:text/html}
