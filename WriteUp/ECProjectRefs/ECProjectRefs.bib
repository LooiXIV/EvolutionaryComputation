@article{olivie-lauquet_release_2001,
	title = {Release of {Trace} {Elements} in {Wetlands}: {Role} of {Seasonal} {Variability}},
	volume = {35},
	issn = {0043-1354},
	shorttitle = {Release of {Trace} {Elements} in {Wetlands}},
	url = {http://www.sciencedirect.com/science/article/pii/S0043135400003286},
	doi = {10.1016/S0043-1354(00)00328-6},
	abstract = {Dissolved concentrations were determined for Fe, Mn, Al, Cu, Zn, La, U, Th, Cd and As in a wetland and its recipient stream to reveal the effect of seasonal changes in environmental conditions on the cycling and transfer of trace elements at the transition between terrestrial and aquatic ecosystems. These preliminary results from the wetland show marked seasonal changes in dissolved concentration for all elements except Zn and Cu. Concentrations are found to be low until about mid-February and then increase abruptly. The onset of trace element release appears to coincide with a marked decline in redox potential and increase of organic carbon content. Because this decline is itself correlated with a pronounced increase in temperature and dissolved Fe, Mn and organic carbon content, we suggest that the microorganisms which use soil iron and manganese oxy-hydroxides as electron acceptors catalyzed the change in redox conditions and induced an increase of DOC. Temporal changes were also observed in the recipient stream which showed marked positive concentration peaks during stormflow events (except Zn). The seasonal processes occurring in the wetland appear to play a major role in determining the amount of trace elements which are transferred from the wetland to the river.},
	number = {4},
	journal = {Water Research},
	author = {Olivie-Lauquet, Gwenaelle and Gruau, Gerard and Dia, Aline and Riou, Christine and Jaffrezic, Anne and Henin, Odile},
	month = mar,
	year = {2001},
	keywords = {wetlands, dissolved organic carbon, redox conditions, trace elements},
	pages = {943--952},
	file = {ScienceDirect Snapshot:files/877/S0043135400003286.html:text/html}
}

@article{tilman_plant_1997,
	title = {Plant diversity and ecosystem productivity: {Theoretical}  considerations},
	volume = {94},
	issn = {0027-8424},
	shorttitle = {Plant diversity and ecosystem productivity},
	url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC20007/},
	abstract = {Ecosystem processes are thought to depend on both the number and
 identity of the species present in an ecosystem, but mathematical
 theory predicting this has been lacking. Here we present three simple
 models of interspecific competitive interactions in communities
 containing various numbers of randomly chosen species. All three models
 predict that, on average, productivity increases asymptotically with
 the original biodiversity of a community. The two models that address
 plant nutrient competition also predict that ecosystem nutrient
 retention increases with biodiversity and that the effects of
 biodiversity on productivity and nutrient retention increase with
 interspecific differences in resource requirements. All three models
 show that both species identity and biodiversity simultaneously
 influence ecosystem functioning, but their relative importance varies
 greatly among the models. This theory reinforces recent experimental
 results and shows that effects of biodiversity on ecosystem functioning
 are predicted by well-known ecological processes.},
	number = {5},
	journal = {Proceedings of the National Academy of Sciences of the United States of America},
	author = {Tilman, David and Lehman, Clarence L. and Thomson, Kendall T.},
	month = mar,
	year = {1997},
	pmid = {11038606},
	pmcid = {PMC20007},
	pages = {1857--1861},
	file = {PubMed Central Full Text PDF:files/878/Tilman et al. - 1997 - Plant diversity and ecosystem productivity Theore.pdf:application/pdf}
}

@article{engelhardt_effects_2001,
	title = {Effects of macrophyte species richness on wetland ecosystem functioning and services},
	volume = {411},
	copyright = {© 2001 Nature Publishing Group},
	issn = {0028-0836},
	url = {https://www.nature.com/nature/journal/v411/n6838/full/411687a0.html},
	doi = {10.1038/35079573},
	abstract = {Wetlands provide many important ecosystem services to human society, which may depend on how plant diversity influences biomass production and nutrient retention. Vascular aquatic plant diversity may not necessarily enhance wetland ecosystem functioning, however, because competition among these plant species can be strong, often resulting in the local dominance of a single species. Here we have manipulated the species richness of rooted, submerged aquatic plant (macrophyte) communities in experimental wetland mesocosms. We found higher algal and total plant (algal plus macrophyte) biomass, as well as lower loss of total phosphorus, in mesocosms with a greater richness of macrophyte species. Greater plant biomass resulted from a sampling effect; that is, the increased chance in species mixtures that algal production would be facilitated by the presence of a less competitive species—in this case, crisped pondweed. Lower losses of total phosphorus resulted from the greater chance in species mixtures of a high algal biomass and the presence of sago pondweed, which physically filter particulate phosphorus from the water. These indirect and direct effects of macrophyte species richness on algal production, total plant biomass and phosphorus loss suggest that management practices that maintain macrophyte diversity may enhance the functioning and associated services of wetland ecosystems.},
	language = {en},
	number = {6838},
	urldate = {2017-06-16},
	journal = {Nature},
	author = {Engelhardt, Katharina A. M. and Ritchie, Mark E.},
	month = jun,
	year = {2001},
	pages = {687--689},
	file = {Snapshot:files/880/411687a0.html:text/html}
}

