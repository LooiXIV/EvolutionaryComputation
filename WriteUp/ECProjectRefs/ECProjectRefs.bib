	file = {Kostopoulou et al. - 2009 - Morphometric changes in a strain of the lineage ‘N.pdf:files/1726/Kostopoulou et al. - 2009 - Morphometric changes in a strain of the lineage ‘N.pdf:application/pdf}
}

