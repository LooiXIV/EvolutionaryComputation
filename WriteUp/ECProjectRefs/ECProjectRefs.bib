	note = {WOS:000304511700015},
	keywords = {spawning habitat, brown trout, chinook   salmon, Dewatering, Fish condition, Fish stranding, habitat use, Hydropower, juvenile atlantic salmon, national-park, northern pike, rapid flow decreases, Regulated rivers, st-lawrence-river, trout salmo-trutta},
	pages = {133--141},
	file = {Nagrodski et al. - 2012 - Fish stranding in freshwater systems Sources, con.pdf:/Users/looi/Zotero/storage/D2MHP7JZ/Nagrodski et al. - 2012 - Fish stranding in freshwater systems Sources, con.pdf:application/pdf}
