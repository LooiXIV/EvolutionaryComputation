@article{tse_wind-driven_2016,
	title = {Wind-driven water motions in wetlands with emergent vegetation},
	volume = {52},
	issn = {1944-7973},
	url = {http://onlinelibrary.wiley.com/doi/10.1002/2015WR017277/abstract},
	doi = {10.1002/2015WR017277},
	abstract = {Wetland biogeochemical transformations are affected by flow and mixing in wetland surface water. We investigate the influence of wind on wetland water flow by simultaneously measuring wind and surface water velocities in an enclosed freshwater wetland during 1 day of strong-wind conditions. Water velocities are measured using a Volumetric Particle Imager while wind velocities are measured via sonic-anemometer. Our measurements indicate that the wind interacting with the vegetation canopy generates coherent billows and that these billows are the dominant source of momentum into the wetland water column. Spectral analysis of velocity time series shows that the spectral peak in water velocity is aligned with the spectral peak of in-canopy wind velocity, and that this peak corresponds with the Kelvin-Helmholtz billow frequency predicted by mixing layer theory. We also observe a strong correlation in the temporal pattern of velocity variance in the air and water, with high variance events having similar timing and duration both above and below the air-water interface. Water-side variance appears coupled with air-side variance at least down to 5 cm, while the theoretical Stokes' solution predicts momentum transfer down to only 2 mm assuming transfer via molecular viscosity alone. This suggests that the wind-driven flow contributed to significant mixing in the wetland water column.},
	language = {en},
	number = {4},
	urldate = {2016-09-13},
	journal = {Water Resources Research},
	author = {Tse, Ian C. and Poindexter, Cristina M. and Variano, Evan A.},
	month = apr,
	year = {2016},
	keywords = {1890 Wetlands, interfacial transfer, mixing, mixing layer, turbulence, wetland},
	pages = {2571--2581},
	file = {Snapshot:/Users/looi/Zotero/storage/P9CVNCTI/abstract.html:text/html;Tse et al. - 2016 - Wind-driven water motions in wetlands with emergen.pdf:/Users/looi/Zotero/storage/2Z29RJMZ/Tse et al. - 2016 - Wind-driven water motions in wetlands with emergen.pdf:application/pdf}
}

@article{hanrahan_attribution_2014,
	title = {Attribution of {Decadal}-{Scale} {Lake}-{Level} {Trends} in the {Michigan}-{Huron} {System}},
	volume = {6},
	copyright = {http://creativecommons.org/licenses/by/3.0/},
	url = {http://www.mdpi.com/2073-4441/6/8/2278},
	doi = {10.3390/w6082278},
	abstract = {This study disentangles causes of the Michigan-Huron system lake-level variability. Regional precipitation is identified as the primary driver of lake levels with sub-monthly time lag, implying that the lake-level time series can be used as a proxy for regional precipitation throughout most of the 1865–present instrumental record. Aside from secular variations associated with the Atlantic Multidecadal Oscillation, the lake-level time series is dominated by two near-decadal cycles with periods of 8 and 12 years. A combination of correlation analysis and compositing suggests that the 8-y cycle stems from changes in daily wintertime precipitation amounts associated with individual storms, possibly due to large-scale atmospheric flow anomalies that affect moisture availability. In contrast, the 12-y cycle is caused by changes in the number of instances, or frequency, of summertime convective precipitation due to a preferred upper-air trough pattern situated over the Great Lakes. In recent decades, the lake-level budget exhibited an abnormal—relative to the remainder of the instrumental record—evaporation-driven trend, likely connected to regional signatures of anthropogenic climate change. The latter effect must be accounted for, along with the effects of precipitation, when assessing possible scenarios of future lake-level variability.},
	language = {en},
	number = {8},
	urldate = {2016-09-13},
	journal = {Water},
	author = {Hanrahan, Janel and Roebber, Paul and Kravtsov, Sergey},
	month = aug,
	year = {2014},
	keywords = {Great Lakes, precipitation, Water levels, climate variability, Lake Michigan-Huron, near-decadal cycles},
	pages = {2278--2299},
	file = {Hanrahan et al. - 2014 - Attribution of Decadal-Scale Lake-Level Trends in .pdf:/Users/looi/Zotero/storage/TN4QJIDG/Hanrahan et al. - 2014 - Attribution of Decadal-Scale Lake-Level Trends in .pdf:application/pdf;Snapshot:/Users/looi/Zotero/storage/QTU5FKRT/2278.html:text/html}
