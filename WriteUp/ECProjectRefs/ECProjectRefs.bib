	month = mar,
	year = {2016},
	keywords = {ecological observatory network, graduate student training, interdisciplinarity, network science, team science},
	pages = {n/a--n/a},
	file = {Read et al. - 2016 - Building the team for team science.pdf:/Users/looi/Zotero/storage/5NTA7CQR/Read et al. - 2016 - Building the team for team science.pdf:application/pdf;Snapshot:/Users/looi/Zotero/storage/MWXMQITV/abstract.html:text/html}
